    "keywords": ["module", "cms", "skeeks", "sx", "app", "yii2", "money"],
    "homepage": "https://cms.skeeks.com",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://www.skeeks.com/",
        "source": "https://github.com/skeeks-cms/cms-money"
    },
    "authors": [
        {
            "name": "Semenov Alexander",
            "email": "semenov@skeeks.com"
        }
    ],
    "require": {
        "skeeks/cms": "~5.0 || dev-master"
    },
    "autoload": {
        "psr-4": {
            "skeeks\\cms\\money\\": "src/"
        }
    },
    "extra": {
